{
    "Name": "Auto Matchmaking Mod",
    "Description": "A mod for vanilla Titanfall 2 that makes matchmaking a lot easier\n\nMade by TimeIsUnending",
    "Version": "1.0.3",

    "LoadPriority": "0",

    "ConVars": [
        {
            "Name": "am_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "am_matchmaking_running",
            "DefaultValue": "0"
        }
    ],

    "Scripts": [
        {
            "Path": "am.gnut",
            "RunOn": "CLIENT && LOBBY",
            "ClientCallback": {
                "After": "AutoMatchmaking_Init"
            }
        }
    ]
}